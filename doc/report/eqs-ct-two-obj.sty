% CTP8, Deb 2001, p368/373, fig 232 ==================================================================

\newcommand{\CTPeight}{
\begin{align}
c_1      &= \cos{(\theta_1)} \, (f_{1}-e) - \sin{(\theta_1)} \, f_{0} \ACR
c_2      &= \sin{(\theta_1)} \, (f_{1}-e) + \cos{(\theta_1)} \, f_{0} \ACR
c_3      &= \sin{(b \, \pi \, (c_2)^c)}                               \ACR
g_{0}    &= c_1 - a \, (\left|c_3\right|)^d                           \ACR
d_1      &= \cos{(\theta_2)} \, (f_{1}-E) - \sin{(\theta_2)} \, f_{0} \ACR
d_2      &= \sin{(\theta_2)} \, (f_{1}-E) + \cos{(\theta_2)} \, f_{0} \ACR
d_3      &= \sin{(B  \, \pi \, (d_2)^C)}                              \ACR
g_{1}    &= d_1 - A \, (\left|d_3\right|)^D
\end{align}
}

