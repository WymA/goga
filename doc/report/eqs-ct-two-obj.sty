% TNK, Deb 2001, p366, fig 224 ======================================================================

\newcommand{\TNK}{
\begin{align}
f_{0} &= x_{0}                                                               \ACR
f_{1} &= x_{1}                                                               \ACR
g_{0} &= x_{0}^2 + x_{1}^2 - 1 - 0.1 \, \cos{[16 \, \atan{(x_{0} / x_{1})}]} \ACR
g_{1} &= 0.5 - (x_{0}-0.5)^2 - (x_{1}-0.5)^2
\end{align}
}

\newcommand{\TNKx}{
\begin{equation}
0 \leq x_i \leq \pi \quad (i \in [0,1])
\end{equation}
}

% CTP1, Deb 2001, p367, fig 225 =====================================================================

\newcommand{\CTPx}{
\begin{equation}
0 \leq x_i \leq 1 \quad (i \in [0,N_x-1])
\end{equation}
}

\newcommand{\CTPone}{
\begin{align}
c_0   &= 1 + \textstyle{\sum_{i=1}^{N_x-1} x_{i}} \ACR
f_{0} &= x_{0}                                    \ACR
f_{1} &= c_0  \,  \exp{(-x_{0}/c_0)}              \ACR
g_{0} &= f_{1} - a_0 \, \exp{(-b_0 \, f_{0})}     \ACR
g_{1} &= f_{1} - a_1 \, \exp{(-b_1 \, f_{0})}
\end{align}
}

\newcommand{\CTPoneC}{
\begin{equation}
a_0 = 0.858 \quad
b_0 = 0.541 \quad
a_1 = 0.728 \quad
b_1 = 0.295
\end{equation}
}

% CTPi, Deb 2001, p368 ===============================================================================

\newcommand{\CTPi}{
\begin{align}
c_0 &= 1 + \textstyle{\sum_{i=1}^{N_x-1} x_{i}}          \ACR
f_0 &= x_0                                               \ACR
f_1 &= c_0  \,  (1 - f_0/c_0)                            \ACR
c_1 &= \cos{(\theta)} \, (f_1-e) - \sin{(\theta)} \, f_0 \ACR
c_2 &= \sin{(\theta)} \, (f_1-e) + \cos{(\theta)} \, f_0 \ACR
c_3 &= \sin{(b \, \pi \, (c_2)^c)}                       \ACR
g_0 &= c_1 - a \, (\left| c_3 \right|)^d
\label{eqn:CTPi}
\end{align}
}

