% FON (Fonseca and Fleming 1995), Deb 2001, p339 ====================================================

\newcommand{\FON}{
\begin{\TWOOBJSIZE}
\begin{align}
f_{0} &= 1 - \exp{\left[- \sum_{i=0}^{N_x-1} \left(x_{i}-\frac{1}{N_x}\right)^2 \right]} \ACR
f_{1} &= 1 - \exp{\left[- \sum_{i=0}^{N_x-1} \left(x_{i}+\frac{1}{N_x}\right)^2 \right]}
\end{align}
\end{\TWOOBJSIZE}
}

\newcommand{\FONx}{
\begin{\TWOOBJSIZE}
\begin{equation}
0 \leq x_i \leq 1 \qquad (i \in [0,N_x-1])
\end{equation}
\end{\TWOOBJSIZE}
}

\newcommand{\FONfront}{
\begin{\TWOOBJSIZE}
\begin{equation}
f_1^{ana}(f_0) = 1 - \exp{\left\{ -\left[2-\sqrt{-\log{(1-f_0)}}\right]^2 \right\}}
\end{equation}
\end{\TWOOBJSIZE}
}

\newcommand{\FONarcLen}{
L_{ref} \approx 1.458
}
