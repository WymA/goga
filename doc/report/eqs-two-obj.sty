\newcommand{\TWOOBJSIZE}{small}

% ZDT1, Deb 2001, p356 ==============================================================================

\newcommand{\ZDTone}{
\begin{\TWOOBJSIZE}
\begin{align}
f_{0} &= x_{0}                                                       \ACR
c_0   &= 1 + \frac{9}{N_x-1} \, \textstyle{\sum_{i=1}^{N_x-1} x_{i}} \ACR
c_1   &= 1 - \sqrt{f_{0}/c_0}                                        \ACR
f_{1} &= c_0  \,  c_1
\end{align}
\end{\TWOOBJSIZE}
}

\newcommand{\ZDToneX}{
\begin{\TWOOBJSIZE}
\begin{equation}
0 \leq x_i \leq 1 \quad (i \in [0,N_x-1])
\end{equation}
\end{\TWOOBJSIZE}
}

\newcommand{\ZDToneFront}{
\begin{\TWOOBJSIZE}
\begin{equation}
f_1^{ana}(f_0) = 1 - \sqrt{f_0}
\end{equation}
\end{\TWOOBJSIZE}
}

\newcommand{\ZDToneArcLen}{
L_{ref} = \sqrt{5}/2 + \log{(\sqrt{5}+2)}/4 \approx 1.478943
}

