% ZDT6, Deb 2001, p360 ==============================================================================

\newcommand{\ZDTsix}{
\begin{\TWOOBJSIZE}
\begin{align}
f_{0} &= 1 - \exp{(-4 \, x_{0})} \, \left[\sin{(6\,\pi \, x_{0})}\right]^6      \ACR
c_0   &= 1 + 9 \, \left(\frac{1}{N_x-1}\,\sum_{i=1}^{N_x-1} x_{i}\right)^{0.25} \ACR
c_1   &= 1 - (f_{0}/c_0)^2                                                      \ACR
f_{1} &= c_0  \,  c_1
\label{eqn:ZDTsix}
\end{align}
\end{\TWOOBJSIZE}
}

